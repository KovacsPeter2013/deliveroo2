{
	"fixed": {
		"style": {
			"position": "fixed"
		}
	},
	"absolute": {
		"style": {
			"position": "absolute"
		}
	},
	"relative": {
		"style": {
			"position": "relative"
		}
	},
	"block": {
		"style": {
			"display": "block"
		}
	},
	"inline": {
		"style": {
			"display": "inline"
		}
	},
	"flex": {
		"style": {
			"display": "flex"
		}
	},
	"hidden": {
		"style": {
			"display": "none"
		}
	},
	"h-7": {
		"style": {
			"height": 28
		}
	},
	"w-7": {
		"style": {
			"width": 28
		}
	},
	"rounded-full": {
		"style": {
			"borderTopLeftRadius": 9999,
			"borderTopRightRadius": 9999,
			"borderBottomRightRadius": 9999,
			"borderBottomLeftRadius": 9999
		}
	},
	"border": {
		"style": {
			"borderTopWidth": 1,
			"borderRightWidth": 1,
			"borderBottomWidth": 1,
			"borderLeftWidth": 1
		}
	},
	"bg-gray-300": {
		"style": {
			"--tw-bg-opacity": 1,
			"backgroundColor": "rgb(209 213 219 / var(--tw-bg-opacity))"
		}
	},
	"text-xs": {
		"style": {
			"fontSize": 12,
			"lineHeight": 16
		}
	},
	"text-xl": {
		"style": {
			"fontSize": 20,
			"lineHeight": 28
		}
	},
	"font-bold": {
		"style": {
			"fontWeight": "700"
		}
	},
	"uppercase": {
		"style": {
			"textTransform": "uppercase"
		}
	},
	"capitalize": {
		"style": {
			"textTransform": "capitalize"
		}
	},
	"text-red-500": {
		"style": {
			"--tw-text-opacity": 1,
			"color": "rgb(239 68 68 / var(--tw-text-opacity))"
		}
	},
	"text-gray-400": {
		"style": {
			"--tw-text-opacity": 1,
			"color": "rgb(156 163 175 / var(--tw-text-opacity))"
		}
	}
}